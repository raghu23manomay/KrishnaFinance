
@model KrishnaFinance.Models.MasterSetting

<script src="~/Scripts/jquery-2.1.1.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
<script src="~/Scripts/jquery-1.10.2.min.js"></script>

<div class="row">
    <div class="col-lg-8">
        <div class="ibox-title">
            <h5>Settings</h5>

        </div>
        <div class="ibox-content">
            <form class="form-horizontal">
                <input type="text" placeholder="Duration" class="form-control hidden" id="SettingID" value="@Model.SettingID">
                <div class="form-group">
                    <label class="col-lg-3">Loan Duration</label>
                    <div class="col-lg-9">
                        <input type="text" placeholder="Duration" class="form-control" id="Duration" value="@Model.Duration">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-3">Processing Fees</label>
                    <div class="col-lg-9">
                        <input type="text" placeholder="ProcessingFees" class="form-control" id="ProcessingFees" value="@Model.ProcessingFees">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-3">Interest Rate</label>
                    <div class="col-lg-9">
                        <input type="text" placeholder="Interest Rate" class="form-control" id="InterestRate" value="@Model.InterestRate">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-3">Interest Amount</label>
                    <div class="col-lg-9">
                        <input type="text" placeholder="Interest Amount" class="form-control" id="InterestAmount" value="@Model.InterestAmount">
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-lg-3">EMI Due Penalty</label>
                    <div class="col-lg-9">
                        <input type="text" placeholder="EMI Due Penalty" class="form-control" id="EMIDuePenalty" value="@Model.EMIDuePenalty">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-3">EMI Due Amount</label>
                    <div class="col-lg-9">
                        <input type="text" placeholder="EMI Due Amount" class="form-control" id="EMIDueAmount" value="@Model.EMIDueAmount">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-3">GST On Disbursement</label>
                    <div class="col-lg-9">
                        <input type="text" placeholder="GST On Disbursement" class="form-control" id="GSTOnDisbursement" value="@Model.GSTOnDisbursement">
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-lg-3">CGST </label>
                    <div class="col-lg-9">
                        <input type="text" placeholder="GST On Disbursement" class="form-control" id="CGST" value="@Model.CGST">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-3">SGST </label>
                    <div class="col-lg-9">
                        <input type="text" placeholder="GST On Disbursement" class="form-control" id="SGST" value="@Model.SGST">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-3">Service Charges</label>
                    <div class="col-lg-9">
                        <input type="text" placeholder="Service Charges" class="form-control" id="ServiceCharges" value="@Model.ServiceCharges">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-3">Pre Closing Charges</label>
                    <div class="col-lg-9">
                        <input type="text" placeholder="Pre Closing Charges" class="form-control" id="PreClosingCharges" value="@Model.PreClosingCharges">
                    </div>
                </div>
                <div class="row form-group">

                    <div class="col-md-8 pull-right">
                        <div class="pull-right">
                            <a class="btn btn-white" href="/Master/ApplicationList">Cancel</a>
                            <button class="btn btn-primary" id="btnsave" type="button">Save </button>
                        </div>
                    </div>
                </div>

            </form>
        </div>
    </div>
</div>

<script>
    $(document).ready(function () {

        $('#btnsave').on('click', function () {

            $.ajax({
                type: "POST",
                cache: false,
                url: "/Master/InsertMasterSetting",
                data: {
                    SettingID: $("#SettingID").val(),
                    Duration: $("#Duration").val(),
                    EMIDuePenalty: $("#EMIDuePenalty").val(),
                    EMIDueAmount: $("#EMIDueAmount").val(),
                    ProcessingFees: $("#ProcessingFees").val(),
                    GSTOnDisbursement: $("#GSTOnDisbursement").val(),
                    CGST: $("#CGST").val(),
                    SGST: $("#SGST").val(),
                    ServiceCharges: $("#ServiceCharges").val(),
                    PreClosingCharges: $("#PreClosingCharges").val(),
                    InterestAmount: $("#InterestAmount").val(),
                    InterestRate: $("#InterestRate").val()
                },
                success: function (result) {
                    alert(result);

                },
                error: function (errorData) {
                }
            });

        });
    });

</script>
